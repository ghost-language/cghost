class Coffee {
    constructor() {
        this.name = "coffee";
    }

    printName() {
        print(this.name);
    }
}

class Latte extends Coffee {
    constructor() {
        this.name = "latte";
    }
}

var latte = Latte();
latte.printName();

class CoffeeMaker {
    constructor(coffee) {
        this.coffee = coffee;
    }

    brew() {
        print("Enjoy your cup of " + this.coffee);
        this.coffee = null;
    }
}

var maker = CoffeeMaker("latte");

maker.brew();